<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jhta.mybatis.mapper.OldbooksMapper.jh">
	<select id="getBigcate" resultType="com.jhta.finalproject.jh.vo.SellerBigcategoryVo">
		select * from bigcategory
	</select>
	<select id="getSmallcate" resultType="com.jhta.finalproject.jh.vo.SellerSmallcategoryVo"
		parameterType="int">
		select * from smallcategory where bcatenum=#{bcatenum}
	</select>
</mapper>