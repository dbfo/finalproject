<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" 
"http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>
	<definition name=".main" template="/WEB-INF/views/jimin/layout.jsp">
		<put-attribute name="header" value="/WEB-INF/views/jimin/header.jsp"></put-attribute>		
		<put-attribute name="content" value="/WEB-INF/views/jimin/home.jsp"></put-attribute>		
		<put-attribute name="footer" value="/WEB-INF/views/jimin/footer.jsp"></put-attribute>		
	</definition>
	
	<definition name=".list1" extends=".main">
		<put-attribute name="content" value="/WEB-INF/views/jimin/cate1.jsp"></put-attribute>
	</definition>
	
	<definition name=".*.*.*" extends=".main">		
		<put-attribute name="content" value="/WEB-INF/views/jimin/{1}/{2}/{3}.jsp"></put-attribute>				
	</definition>
	<definition name=".*.*" extends=".main">		
		<put-attribute name="content" value="/WEB-INF/views/jimin/{1}/{2}.jsp"></put-attribute>				
	</definition>
	
</tiles-definitions>































