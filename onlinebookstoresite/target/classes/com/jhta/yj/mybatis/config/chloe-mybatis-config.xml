<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<!-- 연주 -->
		<!-- books -->
		<typeAlias type="com.jhta.finalproject.yj.vo.BooksVO"
			alias="books" />
		<typeAlias type="com.jhta.finalproject.yj.vo.ImgVO"
			alias="img" />
		<typeAlias type="com.jhta.finalproject.yj.vo.BigCategoryVO"
			alias="bctg" />
		<typeAlias
			type="com.jhta.finalproject.yj.vo.SmallCategoryVO" alias="smctg" />

		<!-- members -->
		<typeAlias type="com.jhta.finalproject.yj.vo.MembersListVO"
			alias="members" />

		<!-- 유래 -->
		<typeAlias type="com.jhta.finalproject.yr.vo.PaymentVo"
			alias="payment" />
		<typeAlias type="com.jhta.finalproject.yr.vo.PaymentBooksVo"
			alias="booksVo" /> 
		<typeAlias type="com.jhta.finalproject.yr.vo.DepositVo"
			alias="depositVo" /> 
		<typeAlias type="com.jhta.finalproject.yr.vo.PointVo"
			alias="pointVo" /> 
		<typeAlias type="com.jhta.finalproject.yr.vo.SettlementJoinVo"
			alias="SettlementJoinVo" /> 
	</typeAliases>
	<mappers>
		<!-- 연주 -->
		<mapper resource="com/jhta/yj/mybatis/mapper/BooksMapper.xml" />
		<mapper resource="com/jhta/yj/mybatis/mapper/MembersMapper.xml" />

		<!-- 유래 -->
		<mapper resource="com/jhta/yr/mybatis/mapper/PaymentMapper.xml" />
		<mapper resource="com/jhta/yr/mybatis/mapper/ShipMapper.xml" />
		<mapper resource="com/jhta/yr/mybatis/mapper/CSMapper.xml" />
		<mapper resource="com/jhta/yr/mybatis/mapper/DepositMapper.xml" />
		
	</mappers>
</configuration>